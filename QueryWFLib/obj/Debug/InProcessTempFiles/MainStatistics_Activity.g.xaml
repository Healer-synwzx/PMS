      <Flowchart.StartNode>
        <FlowStep x:Name="__ReferenceID0">
          <WriteLine Text="读取配置文件" />
          <FlowStep.Next>
            <FlowStep x:Name="__ReferenceID5">
              <local:ReadConfig_Code Id_list_msgid="{x:Null}" Text="{x:Null}" />
